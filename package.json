{
  "name": "campin_react_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "8.13.0",
